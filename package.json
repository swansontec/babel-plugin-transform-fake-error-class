{
  "name": "babel-plugin-transform-fake-error-class",
  "version": "0.1.0",
  "private": true,
  "description": "Transform error classes into error-returning functions",
  "license": "MIT",
  "author": "William Swanson <swansontec@gmail.com>",
  "main": "lib/index.js",
  "scripts": {
    "fix": "npm run lint -- --fix",
    "lint": "eslint --ext .js,.ts .",
    "prepare": "rimraf lib && sucrase ./src -q -d ./lib -t typescript,imports",
    "test": "mocha -r sucrase/register test/**/*.test.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test"
    }
  },
  "lint-staged": {
    "*.{js,ts}": "eslint"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@types/babel__core": "^7.1.3",
    "@types/babel__traverse": "^7.0.8",
    "@types/chai": "^4.2.5",
    "@types/mocha": "^5.2.7",
    "@typescript-eslint/eslint-plugin": ">=2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "chai": "^4.2.0",
    "eslint": ">=6.2.2",
    "eslint-config-standard-kit": ">=0.14.2",
    "eslint-plugin-import": ">=2.18.0",
    "eslint-plugin-prettier": ">=3.0.0",
    "eslint-plugin-promise": ">=4.2.1",
    "eslint-plugin-simple-import-sort": ">=4.0.0",
    "eslint-plugin-standard": ">=4.0.0",
    "husky": ">=3.0.0",
    "lint-staged": ">=9.0.0",
    "mocha": "^6.2.2",
    "prettier": ">=1.0.0",
    "sucrase": "^3.10.1",
    "typescript": "^3.7.2"
  }
}
