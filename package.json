{
  "name": "babel-plugin-transform-fake-error-class",
  "version": "0.1.1",
  "description": "Transform error classes into error-returning functions",
  "license": "MIT",
  "author": "William Swanson <swansontec@gmail.com>",
  "main": "lib/index.js",
  "files": [
    "/CHANGELOG.md",
    "/lib/*",
    "/package.json",
    "/README.md",
    "/src/*"
  ],
  "scripts": {
    "fix": "npm run lint -- --fix",
    "lint": "eslint --ext .js,.ts .",
    "prepare": "rimraf lib && sucrase ./src -q -d ./lib -t typescript,imports",
    "test": "mocha -r sucrase/register test/**/*.test.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test"
    }
  },
  "lint-staged": {
    "*.{js,ts}": "eslint"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@types/babel__core": "^7.1.9",
    "@types/babel__traverse": "^7.0.13",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.23",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^3.6.1",
    "chai": "^4.2.0",
    "eslint": "^7.4.0",
    "eslint-config-standard-kit": "^0.14.4",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "mocha": "^8.0.1",
    "prettier": "^2.0.5",
    "sucrase": "^3.15.0",
    "typescript": "^3.9.7"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0"
  }
}
